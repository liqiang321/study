##指定注册中心地址
#eureka:
#  client:
#    serviceUrl:
#      defaultZone: http://localhost:1111/eureka/
#
##服务的名称
#spring:
#  application:
#    name: product-service
#  #指定从哪个配置中心读取
#  cloud:
#    config:
#      discovery:
#        service-id: config-server-jdbc
#        enabled: true
#      profile: dev
#      label: dev
#
#server:
#  port: 8001